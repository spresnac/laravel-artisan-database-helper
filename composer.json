    "illuminate/console": "^6|^7|^8|^9",
    "symfony/process": "^4.2|^5|^6"
  },
  "autoload": {
    "psr-4": {
      "spresnac\\": "src/spresnac/"
    }
  },
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Sascha Presnac",
      "email": "presnac@gmail.com"
    }
  ],
  "funding": [
    {
      "type": "other",
      "url": "https://www.buymeacoffee.com/spresnac"
    },
    {
      "type": "paypal",
      "url": "https://www.paypal.me/paladin"
    },
    {
      "type": "github",
      "url": "https://github.com/sponsors/spresnac"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "spresnac\\databasehelper\\ArtisanDatabaseHelperServiceProvider"
      ]
    }
  }
}